<%- include("./partials/header") %> 

<div class="container main">
    <div class="row">
        <div class="col-3"></div>
        <div class="col-6">
            <div class="card">
                <div class="card-body">
                  <h5 class="card-title">Covid Cases India</h5>
                  <form method="POST" action="/show">
                  <div class="form-group">
                    <select name="State" class="custom-select" id="s1">
                      <% (Object.keys(body)).forEach((key)=>{ %>  
                        <option value="<%= key %>"><%= key %></option>
                      <% }); %>
                    </select>
                  </div>
                  <div class="form-group">
                    <label for="District">District</label>
                    <input type="text" class="form-control" id="District" placeholder="District" name="District">
                  </div>
                  <button type="submit" class="btn btn-primary">Info</button>
                </form>
                </div>
            </div>
        </div>
    </div>
</div>


<%- include("./partials/footer") %> 
